<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:lastModified="{Date}2018-06-13T14:13:57.485-04:00"
        cq:lastModifiedBy="admin"
        cq:template="/conf/edc/settings/wcm/templates/event-detail-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="edc/components/structure/event/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/structure/header"
                editable="{Boolean}true"/>
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <cq:responsive jcr:primaryType="nt:unstructured"/>
                <eventcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/structure/article/articlecontainer">
                    <messagebanner
                        jcr:created="{Date}2019-08-23T15:30:15.304-06:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2019-08-23T15:30:15.304-06:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/messagebanner"/>
                    <pageherobanner
                        jcr:created="{Date}2018-06-13T14:13:10.736-04:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2018-06-13T14:13:10.736-04:00"
                        jcr:lastModifiedBy="admin"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/pageherobanner"
                        editable="{Boolean}true"/>
                    <eventleftcontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="3"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </eventleftcontainer>
                    <eventbodycontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/structure/article/articlebodycontainer"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="6"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </eventbodycontainer>
                    <eventrightcontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/structure/article/articlerightcontainer"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="3"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </eventrightcontainer>
                    <webinarcta
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/event/webinarcta">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="3"
                                width="6"/>
                        </cq:responsive>
                    </webinarcta>
                    <disclaimerfragment
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/article/disclaimerfragment">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                offset="3"
                                width="6"/>
                        </cq:responsive>
                    </disclaimerfragment>
                </eventcontainer>
                <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"
                    editable="{Boolean}true">
                    <cq:responsive jcr:primaryType="nt:unstructured"/>
                </responsivegrid>
            </responsivegrid>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/structure/footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}719"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}959"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
