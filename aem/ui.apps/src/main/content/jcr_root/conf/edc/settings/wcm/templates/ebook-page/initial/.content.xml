<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:contextHubPath="/etc/cloudsettings/default/contexthub"
        cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
        cq:template="/conf/edc/settings/wcm/templates/ebook-page"
        jcr:primaryType="cq:PageContent"
        sling:redirect="{Boolean}true"
        sling:resourceType="edc/components/structure/ebook/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <ebookcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/structure/article/articlecontainer">
                    <ebookleftcontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"/>
                    <ebookbodycontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/structure/article/articlebodycontainer"/>
                    <pageherobanner
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/pageherobanner"/>
                </ebookcontainer>
                <responsivegrid
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid"/>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
