<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:allowedTemplates="/conf/edc/settings/wcm/templates/success-story-page"
        cq:lastReplicationAction="Activate"
        cq:tags="[edc:format-type/success-story]"
        cq:template="/conf/edc/settings/wcm/templates/success-story-page"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="edc/components/structure/successstory/page"
        isfirst="false"
        publishdate="nodate">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <articlecontainer jcr:primaryType="nt:unstructured">
                    <articlerightcontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/structure/article/articlerightcontainer">
                        <companyataglance
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="edc/components/content/successstory/companyataglance"/>
                    </articlerightcontainer>
                </articlecontainer>
            </responsivegrid>
        </root>
    </jcr:content>
</jcr:root>
