<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/edc/settings/wcm/templates/ehh-answer-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="edc/components/structure/ehh/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/structure/header"/>
            <ehhanswercontainer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/structure/article/articlecontainer">
                <tagresultheader
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/content/article/tagresultheader"/>
                <breadcrumb
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/content/breadcrumb"/>
                <ehhanswerleftcontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/responsivegrid">
                    <articleauthors
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/article/articleauthors"/>
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="1"
                            width="2"/>
                        <tablet
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                    </cq:responsive>
                </ehhanswerleftcontainer>
                <ehhanswerbodycontainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/structure/article/articlebodycontainer">
                    <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/content/text">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </text>
                    <content
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </content>
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            width="6"/>
                        <tablet
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                    </cq:responsive>
                </ehhanswerbodycontainer>
                <ehhwidecontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                    </cq:responsive>
                </ehhwidecontainer>
                <ehhanswercontainer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="edc/components/structure/article/articlebodycontainer">
                    <content
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/foundation/components/responsivegrid"
                        editable="{Boolean}true">
                        <cq:responsive jcr:primaryType="nt:unstructured">
                            <default
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <tablet
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                            <phone
                                jcr:primaryType="nt:unstructured"
                                width="12"/>
                        </cq:responsive>
                    </content>
                    <cq:responsive jcr:primaryType="nt:unstructured">
                        <default
                            jcr:primaryType="nt:unstructured"
                            offset="3"
                            width="6"/>
                        <tablet
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                        <phone
                            jcr:primaryType="nt:unstructured"
                            width="12"/>
                    </cq:responsive>
                </ehhanswercontainer>
            </ehhanswercontainer>
            <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/structure/footer"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}719"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}959"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
