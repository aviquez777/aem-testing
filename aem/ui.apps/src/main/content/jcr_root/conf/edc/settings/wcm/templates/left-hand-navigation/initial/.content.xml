<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/edc/settings/wcm/templates/left-hand-navigation"
        jcr:primaryType="cq:PageContent"
        sling:redirect="{Boolean}true"
        sling:resourceType="edc/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <lefthandnavigation
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="edc/components/content/lefthandnavigation"/>
            </responsivegrid>
            <breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/content/breadcrumb"/>
            <centercolumn
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
            <tagresultheader
                jcr:primaryType="nt:unstructured"
                sling:resourceType="edc/components/content/article/tagresultheader"/>
            <responsivegrid_fullwidth
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"/>
        </root>
    </jcr:content>
</jcr:root>
