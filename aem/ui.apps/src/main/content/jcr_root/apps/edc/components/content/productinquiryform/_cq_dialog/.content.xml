<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Product Inquiry Form"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <authorhelpcomp
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/text"
                text="Secondary titles and text paragraphs authored at component level (Main titles, labels and placeholders in policy)"
                margin="{Boolean}true"/>
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <formTab jcr:primaryType="nt:unstructured"
                        jcr:title="Form"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <form
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/productinquiryform/cq:dialog/form/content"/>
                        </items>
                    </formTab>
                    <step1Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/productinquiryform/cq:dialog/step1/content"/>
                        </items>
                    </step1Tab>
                    <step2Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/productinquiryform/cq:dialog/step2/content"/>
                        </items>
                    </step2Tab>
                    <step3Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/productinquiryform/cq:dialog/step3/content"/>
                        </items>
                    </step3Tab>
                    <formSuccessTab jcr:primaryType="nt:unstructured"
                        jcr:title="Success screen"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <formSuccess
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/productinquiryform/cq:dialog/form-success/content"/>
                        </items>
                    </formSuccessTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
