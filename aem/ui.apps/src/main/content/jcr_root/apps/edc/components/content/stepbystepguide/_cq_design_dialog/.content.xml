<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Step by Step Guide to Export"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <title
                jcr:primaryType="nt:unstructured"
                jcr:title="TITLE"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <overalltitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="TITLE"
                                        name="./title"
                                        required="{Boolean}true"/>
                                </items>
                            </overalltitle>
                        </items>
                    </column>
                </items>
            </title>
            <steps
                jcr:primaryType="nt:unstructured"
                jcr:title="STEPS"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="STEP ONE"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <subtitle1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Subtitle for step one"
                                                name="./subtitle1"
                                                required="{Boolean}true"/>
                                            <text1
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Column text for step one "
                                                name="./text1"
                                                required="{Boolean}true"/>
                                        </items>
                                    </well>
                                </items>
                            </step1>
                            <step2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="STEP TWO"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <subtitle2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Subtitle for step two"
                                                name="./subtitle2"
                                                required="{Boolean}false"/>
                                            <text2
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Column text for step two"
                                                name="./text2"
                                                required="{Boolean}false"/>
                                        </items>
                                    </well>
                                </items>
                            </step2>
                            <step3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="STEP THREE"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <subtitle3
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Subtitle for step three"
                                                name="./subtitle3"
                                                required="{Boolean}false"/>
                                            <text3
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Column text for step three"
                                                name="./text3"
                                                required="{Boolean}false"/>
                                        </items>
                                    </well>
                                </items>
                            </step3>
                            <step4
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="STEP FOUR"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <subtitle4
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Subtitle for step four"
                                                name="./subtitle4"
                                                required="{Boolean}false"/>
                                            <text4
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Column text for step four"
                                                name="./text4"
                                                required="{Boolean}false"/>
                                        </items>
                                    </well>
                                </items>
                            </step4>
                        </items>
                    </column>
                </items>
            </steps>
            <disclaimer
                jcr:primaryType="nt:unstructured"
                jcr:title="DISCLAIMER"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <disclaimer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="DISCLAIMER"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <disclaimer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Disclaimer text"
                                                name="./disclaimer"
                                                required="{Boolean}false"/>
                                        </items>
                                    </well>
                                </items>
                            </disclaimer>
                            <cta
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="CTA"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <linktext
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Link text"
                                                name="./linkText"/>
                                            <linkurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Link url"
                                                name="./linkUrl"
                                                required="{Boolean}false"
                                                rootPath="/content/edc"/>
                                            <linktarget
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Link target"
                                                name="./linkTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <new
                                                        jcr:primaryType="nt:unstructured"
                                                        text="New window"
                                                        value="_blank"/>
                                                    <existing
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Existing window"
                                                        value="_self"/>
                                                </items>
                                            </linktarget>
                                        </items>
                                    </well>
                                </items>
                            </cta>
                        </items>
                    </column>
                </items>
            </disclaimer>
        </items>
    </content>
</jcr:root>
