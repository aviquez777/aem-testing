                                                    <copyright
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#169;"/>
                                                    <trademark
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#8482;"/>
                                                    <registered
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#174;"/>
                                                    <emDash
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#8212;"/>
                                                    <pound
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#163;"/>
                                                    <nbsp
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#160;"/>
