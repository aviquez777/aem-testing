<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured" jcr:title="Step 5"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
             sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <stepbluetitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 5 progress title"
                name="./step5bluetitle"
                required="{Boolean}true"/>
            <steptitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 5 title"
                name="./step5title"
                required="{Boolean}true"/>
            <stepdescription
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                fieldLabel="Step 5 description"
                name="./step5description"/>
            <inelegibleclientannualsales
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/text"
                        class="coral-Form-fieldlabel"
                        text="Inelegible client annual sales"/>
                    <well
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                        <items jcr:primaryType="nt:unstructured">
                            <label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Label"
                                name="./inelegibleclientannualsalesLabel"
                                required="{Boolean}true"/>
                            <secondarylabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Secondary label"
                                name="./inelegibleclientannualsalesSecondaryLabel"/>
                            <help
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Placeholder"
                                name="./inelegibleclientannualsalesHelp"/>
                        </items>
                    </well>
                </items>
            </inelegibleclientannualsales>
            <inelegibleclientindustries
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/text"
                        class="coral-Form-fieldlabel"
                        text="Inelegible client industries"/>
                    <well
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                        <items jcr:primaryType="nt:unstructured">
                            <label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Label"
                                name="./inelegibleclientindustriesLabel"
                                required="{Boolean}true"/>
                            <help
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Placeholder"
                                name="./inelegibleclientindustriesHelp"/>
                            <confirm
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Confirm button"
                                name="./inelegibleclientindustriesConfirm"
                                required="{Boolean}true"/>
                            <clear
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Clear all"
                                name="./inelegibleclientindustriesClear"
                                required="{Boolean}true"/>
                            <singularoption
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Singular option selected"
                                name="./inelegibleclientindustriesSingular"
                                required="{Boolean}true"/>
                            <pluraloptions
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Plural option selected"
                                name="./inelegibleclientindustriesPlural"
                                required="{Boolean}true"/>
                        </items>
                    </well>
                </items>
            </inelegibleclientindustries>
            <inelegibleclientshipment
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/text"
                    class="coral-Form-fieldlabel"
                    text="Inelegible client range of shipments"/>
                    <well
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                        <items jcr:primaryType="nt:unstructured">
                            <label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Label"
                                name="./inelegibleclientshipmentLabel"
                                required="{Boolean}true"/>
                            <help
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Placeholder"
                                name="./inelegibleclientshipmentHelp"/>
                        </items>
                    </well>
                </items>
            </inelegibleclientshipment>
            <inelegibleclientother
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <heading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/text"
                        class="coral-Form-fieldlabel"
                        text="Inelegible Other"/>
                    <well
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                        <items jcr:primaryType="nt:unstructured">
                            <label
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Label"
                                name="./inelegibleclientotherLabel"
                                required="{Boolean}true"/>
                            <help
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Placeholder"
                                name="./inelegibleclientotherHelp"/>
                        </items>
                    </well>
                </items>
            </inelegibleclientother>
        </items>
    </content>
</jcr:root>