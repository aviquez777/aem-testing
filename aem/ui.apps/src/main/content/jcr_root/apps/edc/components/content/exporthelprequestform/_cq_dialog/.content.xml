<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Export help request form"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <general
                jcr:primaryType="nt:unstructured"
                jcr:title="General"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <inquiryID
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/inquiryID" />
                            <buttons
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="Button labels"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <nextbutton
                                                jcr:primaryType="cq:Widget"
                                                sling:resourceType="granite/ui/components/foundation/include"
                                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/buttons/items/well/items/nextbutton" />
                                            <submitbutton
                                                jcr:primaryType="cq:Widget"
                                                sling:resourceType="granite/ui/components/foundation/include"
                                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/buttons/items/well/items/submitbutton" />
                                            <donebutton
                                                jcr:primaryType="cq:Widget"
                                                sling:resourceType="granite/ui/components/foundation/include"
                                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/buttons/items/well/items/donebutton" />
                                            <backbutton
                                                jcr:primaryType="cq:Widget"
                                                sling:resourceType="granite/ui/components/foundation/include"
                                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/buttons/items/well/items/backbutton" />
                                        </items>
                                    </well>
                                </items>
                            </buttons>
                            <steplabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="Step # of # label"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <step
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Step"
                                                name="./step"
                                                required="{Boolean}true"/>
                                            <of
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Of"
                                                name="./of"
                                                required="{Boolean}true"/>
                                        </items>
                                    </well>
                                </items>
                            </steplabel>
                            <sucess
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/sucess" />
                            <disclaimer
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/general/items/column/items/disclaimer" />
                        </items>
                    </column>
                </items>
            </general>
            <level1
                jcr:primaryType="nt:unstructured"
                jcr:title="Level 1"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level1/items/column/items/heading"/>
                            <subtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subtitle"
                                name="./subtitleP1"/>
                            <relatedquestion
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <heading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/text"
                                        class="coral-Form-fieldlabel"
                                        text="Related question"/>
                                    <well
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container">
                                        <layout
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                        <items jcr:primaryType="nt:unstructured">
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Label"
                                                name="./questionLabel"
                                                required="{Boolean}true"/>
                                            <help
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Placeholder"
                                                name="./questionPlaceholder"
                                                required="{Boolean}false"/>
                                        </items>
                                    </well>
                                </items>
                            </relatedquestion>
                            <email
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level1/items/column/items/email"/>
                        </items>
                    </column>
                </items>
            </level1>
            <level2
                jcr:primaryType="nt:unstructured"
                jcr:title="Level 2"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/heading" />
                            <subtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subtitle"
                                name="./subtitleP2"/>
                            <firstName
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/firstName" />
                            <lastName
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/lastName" />
                            <title
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/title" />
                            <companyname
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/companyname" />
                            <tradestatus
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/tradestatus" />
                            <mainphone
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level2/items/column/items/mainphone" />
                        </items>
                    </column>
                </items>
            </level2>
            <level3
                jcr:primaryType="nt:unstructured"
                jcr:title="Level 3"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/heading" />
                            <subtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subtitle"
                                name="./subtitleP3"/>
                            <companyaddress
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/companyaddress" />
                            <companycountry
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/companycountry" />
                            <companyprovince
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/companyprovince" />
                            <companycity
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/companycity" />
                            <companypostal
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level3/items/column/items/companypostal" />
                        </items>
                    </column>
                </items>
            </level3>
            <level4
                jcr:primaryType="nt:unstructured"
                jcr:title="Level 4"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level4/items/column/items/heading" />
                            <subtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subtitle"
                                name="./subtitleP4"/>
                            <annualsales
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level4/items/column/items/annualsales" />
                            <employees
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level4/items/column/items/employees" />
                            <painpoint
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level4/items/column/items/painpoint" />
                        </items>
                    </column>
                </items>
            </level4>
            <level5
                jcr:primaryType="nt:unstructured"
                jcr:title="Level 5"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level5/items/column/items/heading" />
                            <subtitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Subtitle"
                                name="./subtitleP5"/>
                            <industry
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level5/items/column/items/industry" />
                            <marketsint
                                jcr:primaryType="cq:Widget"
                                sling:resourceType="granite/ui/components/foundation/include"
                                path="/apps/edc/components/content/premium/progressiveprofiling/cq:design_dialog/content/items/level5/items/column/items/marketsint" />
                        </items>
                    </column>
                </items>
            </level5>
        </items>
    </content>
</jcr:root>
