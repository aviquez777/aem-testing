<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="EDC Page"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cq-dialog-content-page"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured">
                <items jcr:primaryType="nt:unstructured">
                    <basic
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured"
                                    sling:hideChildren="[series]">
                                    <costum
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Additional EDC Properties"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <synopsis
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Synopsis"
                                                name="./articlesynopsis"
                                                required="{Boolean}true"
                                                wrapperClass="core-image-alt"/>
                                        </items>
                                    </costum>
                                </items>
                            </column>
                        </items>
                    </basic>
                    <event
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Event Detail"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                cq:showOnCreate="{Boolean}true"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <webinar
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Webinar Details:"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <webinarshowkey
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Webinar Show Key"
                                                fieldLabel="Webinar Show Key"
                                                required="{Boolean}true"
                                                name="./webinarshowkey"/>
                                            <!-- Task 230762 -->
                                            <decomvideourl
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This Webinar's video URL will override the one provided by Arkadin"
                                                fieldLabel= "Decommissioned Webinar's URL"
                                                required="{Boolean}false"
                                                name="./decomvideourl"/>
                                            <webinaroverride
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldDescription="If checked webinar button logic will not take effect"
                                                name="./webinaroverride"
                                                text="Override Webinar logic?"
                                                uncheckedValue="false"
                                                value="true"/>
                                          <!-- change the text and the multifield-limit-min:3-max:3 as necessary -->
                                            <partners
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click '+' to add a new partner"
                                                fieldLabel="Partners and CASLs"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true" 
                                                validation="multifield-limit-min:0-max:5">
                                                <field
                                                      sling:resourceType="granite/ui/components/coral/foundation/container"
                                                      jcr:primaryType="nt:unstructured"
                                                      name="./partners">
                                                      <items jcr:primaryType="nt:unstructured">
                                                        <partner
                                                            cq:showOnCreate="{Boolean}true"
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:orderBefore="redirect"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please follow the naming convention provided"
                                                            fieldLabel="Partner"
                                                            required="{Boolean}true"
                                                            name="./partner"/>
                                                            <casltext
                                                                cq:showOnCreate="{Boolean}true"
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                                fieldDescription="CASL partnership copy (leave blank if no CASL)"
                                                                fieldLabel="CASL Text"
                                                                required="{Boolean}false"
                                                                name="./casltext"
                                                                useFixedInlineToolbar="{Boolean}true">
                                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                                    <format
                                                                        jcr:primaryType="nt:unstructured"
                                                                        features="*"/>
                                                                    <justify
                                                                        jcr:primaryType="nt:unstructured"
                                                                        features="*"/>
                                                                    <links
                                                                        jcr:primaryType="nt:unstructured"
                                                                        features="[modifylink,unlink]"/>
                                                                </rtePlugins>
                                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                                    <cui jcr:primaryType="nt:unstructured">
                                                                        <inline
                                                                            jcr:primaryType="nt:unstructured"
                                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                                <justify
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                                    ref="justify"/>
                                                                            </popovers>
                                                                        </inline>
                                                                    </cui>
                                                                </uiSettings>
                                                           </casltext>
                                                  </items>
                                                </field>
                                            </partners>
                                            <selfidmodaltext
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldLabel="Self-identification intro text (Will show above on top of the Self-identification radio buttons)"
                                                required="{Boolean}false"
                                                name="./selfidmodaltext"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                    <format
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        features="[modifylink,unlink]"/>
                                                </rtePlugins>
                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                    <cui jcr:primaryType="nt:unstructured">
                                                        <inline
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                            </popovers>
                                                        </inline>
                                                    </cui>
                                                </uiSettings>
                                            </selfidmodaltext>
                                            <witradios
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                fieldDescription="Click '+' to add a new WIT Button"
                                                fieldLabel="Self-identification radio buttons"
                                                jcr:primaryType="nt:unstructured"
                                                composite="{Boolean}true" 
                                                validation="multifield-limit-min:0-max:3">
                                                <field
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    jcr:primaryType="nt:unstructured"
                                                    name="./witradios">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <witvalue
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Value"
                                                            name="./witvalue"
                                                            required="{Boolean}true">
                                                            <datasource
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="/apps/edc/components/common/datasource/witlov.html"/>
                                                        </witvalue>
                                                        <radiotext
                                                          cq:showOnCreate="{Boolean}true"
                                                          jcr:primaryType="nt:unstructured"
                                                          sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                          fieldDescription="Desctiption next to the radio"
                                                          fieldLabel="Radio Text"
                                                          required="{Boolean}true"
                                                          name="./radiotext"
                                                          useFixedInlineToolbar="{Boolean}true">
                                                          <rtePlugins jcr:primaryType="nt:unstructured">
                                                              <format
                                                                  jcr:primaryType="nt:unstructured"
                                                                  features="*"/>
                                                              <justify
                                                                  jcr:primaryType="nt:unstructured"
                                                                  features="*"/>
                                                              <links
                                                                  jcr:primaryType="nt:unstructured"
                                                                  features="[modifylink,unlink]"/>
                                                          </rtePlugins>
                                                          <uiSettings jcr:primaryType="nt:unstructured">
                                                              <cui jcr:primaryType="nt:unstructured">
                                                                  <inline
                                                                      jcr:primaryType="nt:unstructured"
                                                                      toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                                      <popovers jcr:primaryType="nt:unstructured">
                                                                          <justify
                                                                              jcr:primaryType="nt:unstructured"
                                                                              items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                              ref="justify"/>
                                                                      </popovers>
                                                                  </inline>
                                                              </cui>
                                                          </uiSettings>
                                                        </radiotext>
                                                    </items>
                                                </field>
                                            </witradios>
                                            <specialmodaltext
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Text for special webinar modal (leave blank for regular modal behavior)"
                                                fieldLabel="Special webinar modal text (Include text for each of the Above Partner's)"
                                                required="{Boolean}false"
                                                name="./specialmodaltext"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                    <format
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        features="[modifylink,unlink]"/>
                                                </rtePlugins>
                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                    <cui jcr:primaryType="nt:unstructured">
                                                        <inline
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                            </popovers>
                                                        </inline>
                                                    </cui>
                                                </uiSettings>
                                            </specialmodaltext>
                                            <specialarkadinmodaltext
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                fieldDescription="Text for special webinar modal, must combine Arkadin and special modal text"
                                                fieldLabel="Arkadin + Special webinar modal text (Include text for each of the Above Partner's)"
                                                required="{Boolean}false"
                                                name="./specialarkadinmodaltext"
                                                useFixedInlineToolbar="{Boolean}true">
                                                <rtePlugins jcr:primaryType="nt:unstructured">
                                                    <format
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        features="*"/>
                                                    <links
                                                        jcr:primaryType="nt:unstructured"
                                                        features="[modifylink,unlink]"/>
                                                </rtePlugins>
                                                <uiSettings jcr:primaryType="nt:unstructured">
                                                    <cui jcr:primaryType="nt:unstructured">
                                                        <inline
                                                            jcr:primaryType="nt:unstructured"
                                                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                            <popovers jcr:primaryType="nt:unstructured">
                                                                <justify
                                                                    jcr:primaryType="nt:unstructured"
                                                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                                    ref="justify"/>
                                                            </popovers>
                                                        </inline>
                                                    </cui>
                                                </uiSettings>
                                            </specialarkadinmodaltext>
                                        </items>
                                    </webinar>
                                    <event
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Event Details:"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <eventid
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                fieldDescription="Event ID"
                                                fieldLabel="Event ID"
                                                name="./eventid"/>
                                            <assetTier
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Asset Tier"
                                                emptyOption="{Boolean}true"
                                                name="./assetTier">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <level1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="1"
                                                        value="1"/>
                                                    <level2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="2"
                                                        value="2"/>
                                                    <level3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="3"
                                                        value="3"/>
                                                    <level4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="4"
                                                        value="4"/>
                                                    <level5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="5"
                                                        value="5"/>
                                                </items>
                                            </assetTier>
                                            <timezone
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Timezone information to display on the event detail page"
                                                fieldLabel="Time Zone"
                                                name="./timezone"
                                                required="{Boolean}true"/>
                                            <startDate
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                displayedFormat="MMMM DD, YYYY"
                                                fieldDescription="Start Date"
                                                fieldLabel="Start Date"
                                                name="./startDate"
                                                required="{Boolean}true"
                                                type="date"
                                                typeHint="Date"/>
                                            <startTime
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                displayedFormat="HH:mm "
                                                fieldDescription="Start Time"
                                                fieldLabel="Start Time"
                                                name="./startTime"
                                                required="{Boolean}true"
                                                type="time"
                                                typeHint="Date"/>
                                            <endDate
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                displayedFormat="MMMM DD, YYYY"
                                                fieldDescription="End Date"
                                                fieldLabel="End Date"
                                                name="./endDate"
                                                required="{Boolean}true"
                                                type="date"
                                                typeHint="Date"/>
                                            <endTime
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                                displayedFormat="HH:mm "
                                                fieldDescription="The End Time field cannot be filled out unless Start Time is also filled out"
                                                fieldLabel="End Time (Must be filled if Start Time is filled out)"
                                                name="./endTime"
                                                required="{Boolean}true"
                                                type="time"
                                                typeHint="Date"/>
                                            <locationName
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Location Name"
                                                fieldLabel="Location Name"
                                                name="./location"/>
                                            <address
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Address"
                                                fieldLabel="Address"
                                                name="./address"/>
                                            <city
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="City"
                                                fieldLabel="City"
                                                name="./city"/>
                                            <province
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Province"
                                                fieldLabel="Province"
                                                name="./province"/>
                                            <country
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Country"
                                                fieldLabel="Country"
                                                name="./country"/>
                                            <mapLink
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Map Link"
                                                fieldLabel="Map Link"
                                                name="./mapLink"/>
                                            <mapLinkTarget
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Target Window To Open Map Link"
                                                name="./mapLinkTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <new
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="New Window"
                                                        value="_blank"/>
                                                    <current
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Current Winow"
                                                        value="_self"/>
                                                </items>
                                            </mapLinkTarget>
                                            <whoAttends
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Who Attends"
                                                name="./whoattends"
                                                wrapperClass="core-image-alt"/>
                                            <relatedWebinar
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Related Webinar"
                                                name="./relatedWebinar"
                                                wrapperClass="core-image-alt"/>
                                            <relatedWebinarLink
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="If the Related Webinar field is filled out, link to Webinar field must also be filled"
                                                fieldLabel="Link To Webinar(Must be filled if Related Webinar is filled out)"
                                                name="./webinarLink"
                                                rootPath="/content/edc"/>
                                            <relatedWebinarLinkTarget
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:orderBefore="redirect"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Target Window To Open Webinar Link"
                                                name="./webinarLinkTarget">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <new
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="New Window"
                                                        value="_blank"/>
                                                    <current
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Current Winow"
                                                        value="_self"/>
                                                </items>
                                            </relatedWebinarLinkTarget>
                                            <registerLink
                                                cq:showOnCreate="{Boolean}true"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                fieldDescription="Link to a page to register for event"
                                                fieldLabel="Event Register Link"
                                                name="./eventRegLink"
                                                rootPath="/content/edc"/>
                                        </items>
                                    </event>
                                </items>
                            </column>
                        </items>
                    </event>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
