<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <step3modals
                jcr:primaryType="nt:unstructured"
                jcr:title="Step 1 Modals"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items
                            jcr:primaryType="nt:unstructured">
                            <tradeNameModalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Trade Name Modal Title"
                                fieldDescription="Trade Name Modal Title"
                                name="./tradeNameModalTitle"
                                required="{Boolean}false"/>
                           <tradeNameModalDescription
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Trade Name Modal Description"
                               fieldDescription="Trade Name Modal Description."
                               name="./tradeNameModalDescription"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </tradeNameModalDescription>
                           <brnModalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Business registration number Modal Title"
                                fieldDescription="Business registration number Modal Title"
                                name="./brnModalTitle"
                                required="{Boolean}false"/>
                           <brnModalDescription
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Business registration number Modal Description"
                               fieldDescription="Business registration number Modal Title"
                               name="./brnModalDescription"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </brnModalDescription>
                           <uboModalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Ultimate beneficial owner Modal Title"
                                fieldDescription="Ultimate beneficial owner Modal Title"
                                name="./uboModalTitle"
                                required="{Boolean}false"/>
                           <uboModalDescription
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Ultimate beneficial owner Modal Description"
                               fieldDescription="Ultimate beneficial owner Modal Description"
                               name="./uboModalDescription"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </uboModalDescription>
                           <hqModalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Headquarter Modal Title"
                                fieldDescription="Headquarter Modal Title"
                                name="./hqModalTitle"
                                required="{Boolean}false"/>
                           <hqModalDescription
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Headquarter Modal Description"
                               fieldDescription="Headquarter Modal Description"
                               name="./hqModalDescription"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </hqModalDescription>
                        </items>
                    </column>
                </items>
            </step3modals>
        </items>
    </content>
</jcr:root>
