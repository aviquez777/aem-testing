<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Step 1"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <step jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 1 of 3 label"
                required="{Boolean}true"
                name="./step1Of3"/>
            <conductchecklabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                fieldLabel="Code of Conduct checkbox label"
                name="./conductchecklabel"
                useFixedInlineToolbar="{Boolean}true"
                required="{Boolean}true">
                <rtePlugins jcr:primaryType="nt:unstructured">
                    <format
                        jcr:primaryType="nt:unstructured"
                        features="*"/>
                    <justify
                        jcr:primaryType="nt:unstructured"
                        features="*"/>
                    <links
                        jcr:primaryType="nt:unstructured"
                        features="[modifylink,unlink]"/>
                </rtePlugins>
                <uiSettings jcr:primaryType="nt:unstructured">
                    <cui jcr:primaryType="nt:unstructured">
                        <inline
                            jcr:primaryType="nt:unstructured"
                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                            <popovers jcr:primaryType="nt:unstructured">
                                <justify
                                    jcr:primaryType="nt:unstructured"
                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                    ref="justify"/>
                            </popovers>
                        </inline>
                    </cui>
                </uiSettings>
            </conductchecklabel>
            <firstnamelabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="First name label"
                name="./firstnamelabel"
                required="{Boolean}true"/>
            <firstnameplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="First name placeholder"
                name="./firstnameplaceholder"
                required="{Boolean}false"/>
            <lastnamelabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Last name label"
                name="./lastnamelabel"
                required="{Boolean}true"/>
            <lastnameplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Last name placeholder"
                name="./lastnameplaceholder"
                required="{Boolean}false"/>
            <emaillabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Email address label"
                name="./emaillabel"
                required="{Boolean}true"/>                            
            <emailplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Email address placeholder"
                name="./emailplaceholder"
                required="{Boolean}false"/>
            <phonelabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Main phone label"
                name="./phonelabel"
                required="{Boolean}true"/>
            <phoneplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Main phone placeholder"
                name="./phoneplaceholder"
                required="{Boolean}false"/>
            <phoneextlabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Phone extension label"
                name="./phoneextlabel"
                required="{Boolean}true"/>
            <phoneextplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Phone extension placeholder"
                name="./phoneextplaceholder"
                required="{Boolean}false"/>
            <faxlabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Fax label"
                name="./faxlabel"
                required="{Boolean}true"/>
            <faxplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Fax placeholder"
                name="./faxplaceholder"
                required="{Boolean}false"/>  
            <typereglabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Type of registration radios label"
                name="./typereglabel"
                required="{Boolean}true"/>
            <referralfirstnamelabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Referral first name label"
                name="./referralfirstnamelabel"
                required="{Boolean}true"/>
            <referralfirstnameplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Referral first name placeholder"
                name="./referralfirstnameplaceholder"
                required="{Boolean}false"/>
            <referrallastnamelabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Referral last name label"
                name="./referrallastnamelabel"
                required="{Boolean}true"/>
            <referrallastnameplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Referral last name placeholder"
                name="./referrallastnameplaceholder"
                required="{Boolean}false"/> 
            <corpstatuslabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Corporate status label"
                name="./corpstatuslabel"
                required="{Boolean}true"/>   
            <corpstatusotherplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Corporate status Other field placeholder"
                name="./corpstatusotherplaceholder"
                required="{Boolean}false"/>
            <businesslabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Business services label"
                name="./businesslabel"
                required="{Boolean}true"/> 
            <businessotherplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Business Other field placeholder"
                name="./businessotherplaceholder"
                required="{Boolean}false"/>
        </items>
    </content>
</jcr:root>