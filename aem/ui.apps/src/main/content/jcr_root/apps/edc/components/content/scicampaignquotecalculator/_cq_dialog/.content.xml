<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="SCI Campaign Quote Calculator"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
        margin="{Boolean}false">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <version
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/libs/granite/ui/components/coral/foundation/form/select"
                        fieldDescription="Select the Form Type"
                        fieldLabel="SCI A/B Quote Calculator form Type"
                        name="./version">
                        <items jcr:primaryType="nt:unstructured">
                            <typeA
                                jcr:primaryType="nt:unstructured"
                                selected="{Boolean}true"
                                text="Form A" 
                                value="a"/>
                            <typeB
                                jcr:primaryType="nt:unstructured"
                                selected="{Boolean}false"
                                text="Form B"
                                value="b"/>
                        </items>
                    </version>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./title"
                        required="{Boolean}true"/>
                    <linkid
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Optional internal link id."
                        fieldLabel="Link ID"
                        name="./linkid"
                        required="{Boolean}false"/>
                    <labels
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/text"
                                text="Calculator labels"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <amounttoinsure
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Amount to insure label text"
                                        name="./amounttoinsure"
                                        required="{Boolean}true"/>
                                    <amounttoinsuremaxvalue
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldLabel="Max Amount value to insure"
                                        fieldDescription="Please adjust the amount on the description accordingly"
                                        required="{Boolean}false"
                                        name="./amounttoinsuremaxvalue"/>
                                    <heading jcr:primaryType="nt:unstructured" 
                                        sling:resourceType="granite/ui/components/foundation/text" 
                                        class="coral-Heading--4"
                                        text="default value is 500000, please update the amount on the textbox above"/>
                                    <amounttoinsureplaceholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Typically &quot;Enter amount (CAD)&quot;"
                                        fieldLabel="Amount to insure placeholder"
                                        name="./amounttoinsureplaceholder"
                                        required="{Boolean}true"/>
                                    <amounttoinsureselected
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Amount to insure label text after selection"
                                        fieldDescription="Amount to insure label on top of selected value, will show after submitting email"
                                        name="./amounttoinsureselected"
                                        required="{Boolean}true"/>
                                    <coverageperiod
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Coverage period label text"
                                        name="./coverageperiod"
                                        required="{Boolean}true"/>
                                    <coverageperiodunitplaceholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Select Duration placeholder"
                                        fieldLabel="Coverage period placeholder"
                                        name="./coverageperiodunitplaceholder"/>
                                    <coverageperiodunit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        description="Typically &quot;days&quot;"
                                        fieldDescription="Typically &quot;days&quot;"
                                        fieldLabel="Coverage period unit"
                                        name="./coverageperiodunit"
                                        required="{Boolean}true"/>
                                    <coverageperiodselected
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Coverage period label text after selection"
                                        fieldDescription="Coverage period label on top of selected value, will show after submitting email"
                                        name="./coverageperiodselected"
                                        required="{Boolean}true"/>
                                    <paymenterms
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Payment terms"
                                        name="./paymentterms"
                                        required="{Boolean}true"/>
                                    <cpaymentermsplaceholder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Select Duration placeholder"
                                        fieldLabel="Payment terms option placeholder"
                                        name="./paymentermsplaceholder"/>
                                    <paymentermsoptionone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Payment terms option text (0 - 90)"
                                        name="./paymenttermsoptionone"
                                        required="{Boolean}true"/>
                                    <paymentermsoptiontwo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Payment terms option text (91- 180)"
                                        name="./paymenttermsoptiontwo"
                                        required="{Boolean}true"/>
                                    <paymentermsselected
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Payment terms label text after selection"
                                        fieldDescription="Payment terms label on top of selected value, will show after submitting email"
                                        name="./paymenttermsselected"
                                        required="{Boolean}true"/>
                                </items>
                            </well>
                        </items>
                    </labels>
                    <messages
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/text"
                                text="Calculator messages"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <zeroamounttoinsure
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Shown when a zero dollar amount is entered into the calculator."
                                        fieldLabel="Zero amount to insure"
                                        name="./zeroamounttoinsure"
                                        required="{Boolean}true"/>
                                    <coverageerrormessage
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Coverage error message"
                                        name="./coverageerrormessage"
                                        required="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <formats jcr:primaryType="nt:unstructured">
                                                    <small
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Small"
                                                        tag="small"/>
                                                    <default_p
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Paragraph"
                                                        tag="p"/>
                                                    <default_h1
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 1"
                                                        tag="h1"/>
                                                    <default_h2
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 2"
                                                        tag="h2"/>
                                                    <default_h3
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 3"
                                                        tag="h3"/>
                                                    <default_h4
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 4"
                                                        tag="h4"/>
                                                    <default_h5
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 5"
                                                        tag="h5"/>
                                                    <default_h6
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 6"
                                                        tag="h6"/>
                                                    <default_blockquote
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Quote"
                                                        tag="blockquote"/>
                                                    <default_pre
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Preformatted"
                                                        tag="pre"/>
                                                </formats>
                                            </paraformat>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <findreplace
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <subsuperscript
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <spellcheck
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <misctools
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                    <chars jcr:primaryType="nt:unstructured">
                                                        <copyright
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#169;"/>
                                                        <trademark
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8482;"/>
                                                        <registered
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#174;"/>
                                                        <emDash
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8212;"/>
                                                        <pound
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#163;"/>
                                                        <nbsp
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#160;"/>
                                                    </chars>
                                                </specialCharsConfig>
                                            </misctools>
                                            <styles
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <styles jcr:primaryType="nt:unstructured">
                                                    <plainSpan
                                                        jcr:primaryType="nt:unstructured"
                                                        cssName="defaultSpan"
                                                        text="Default (add span tag)"/>
                                                    <lead
                                                        jcr:primaryType="nt:unstructured"
                                                        cssName="lead"
                                                        text="Lead"/>
                                                </styles>
                                            </styles>
                                            <table
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <justify
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                            ref="justify"/>
                                                        <lists
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                            ref="lists"/>
                                                        <paraformat
                                                            jcr:primaryType="nt:unstructured"
                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                            ref="paraformat"/>
                                                        <styles
                                                            jcr:primaryType="nt:unstructured"
                                                            items="styles:getStyles:styles-pulldown"
                                                            ref="styles"/>
                                                    </popovers>
                                                </inline>
                                            </cui>
                                        </uiSettings>
                                    </coverageerrormessage>
                                    <coveragesuccess
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Important: text must contain the following code &lt;strong id=&quot;coverage-success-value&quot;&gt; 90 days &lt;/strong&gt;"
                                        fieldLabel="Coverage success message"
                                        name="./coveragesuccess"
                                        required="{Boolean}true"/>
                                    <heading jcr:primaryType="nt:unstructured" 
                                        sling:resourceType="granite/ui/components/foundation/text" 
                                        class="coral-Heading--4"
                                        text="Important: The Coverage success message text must contain the following code: &lt;strong id=&quot;coverage-success-value&quot;&gt; 90 days &lt;/strong&gt;"/>
                                    <disclaimer
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldLabel="Disclaimer message"
                                        name="./disclaimer"
                                        required="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <findreplace
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <subsuperscript
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <spellcheck
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <misctools
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                    <chars jcr:primaryType="nt:unstructured">
                                                        <copyright
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#169;"/>
                                                        <trademark
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8482;"/>
                                                        <registered
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#174;"/>
                                                        <emDash
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8212;"/>
                                                        <pound
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#163;"/>
                                                        <nbsp
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#160;"/>
                                                    </chars>
                                                </specialCharsConfig>
                                            </misctools>
                                            <styles
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <styles jcr:primaryType="nt:unstructured">
                                                    <plainSpan
                                                        jcr:primaryType="nt:unstructured"
                                                        cssName="defaultSpan"
                                                        text="Default (add span tag)"/>
                                                    <lead
                                                        jcr:primaryType="nt:unstructured"
                                                        cssName="lead"
                                                        text="Lead"/>
                                                </styles>
                                            </styles>
                                            <table
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <justify
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                            ref="justify"/>
                                                        <lists
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                            ref="lists"/>
                                                        <paraformat
                                                            jcr:primaryType="nt:unstructured"
                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                            ref="paraformat"/>
                                                        <styles
                                                            jcr:primaryType="nt:unstructured"
                                                            items="styles:getStyles:styles-pulldown"
                                                            ref="styles"/>
                                                    </popovers>
                                                </inline>
                                            </cui>
                                        </uiSettings>
                                    </disclaimer>
                                    <schedule
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Typically &quot;To speak to a Trade Advisor, weekdays, 9 am to 5 pm EST&quot;"
                                        fieldLabel="Service hours"
                                        name="./schedule"
                                        required="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                           <format
                                               jcr:primaryType="nt:unstructured"
                                               features="*"/>
                                           <justify
                                               jcr:primaryType="nt:unstructured"
                                               features="*"/>
                                           <links
                                               jcr:primaryType="nt:unstructured"
                                               features="[modifylink,unlink]"/>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                           <cui jcr:primaryType="nt:unstructured">
                                               <inline
                                                   jcr:primaryType="nt:unstructured"
                                                   toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                   <popovers jcr:primaryType="nt:unstructured">
                                                       <justify
                                                           jcr:primaryType="nt:unstructured"
                                                           items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                           ref="justify"/>
                                                   </popovers>
                                               </inline>
                                           </cui>
                                        </uiSettings>
                                    </schedule>
                                </items>
                            </well>
                        </items>
                    </messages>
                    <cta1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/text"
                                fieldDescription="CTA Reset Label"
                                text="CTA Reset"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <cta1text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Reset Text"
                                        name="./cta1text"/>
                                    <resetFormButtonEnTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Analytics reset Form Button English Text i.e. 'submit step'"
                                        fieldLabel="Analytics Reset Form Button English Text"
                                        name="./resetFormButtonEnTxt"
                                        required="{Boolean}true"/>
                                </items>
                            </well>
                        </items>
                    </cta1>
                    <cta2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/text"
                                fieldDescription="CTA Learn more Label"
                                text="CTA Learn more"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <cta2target
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Learn more Target"
                                        name="./cta2target">
                                        <items jcr:primaryType="nt:unstructured">
                                            <existing
                                                jcr:primaryType="nt:unstructured"
                                                text="Existing window"
                                                value="_self"/>
                                            <new
                                                jcr:primaryType="nt:unstructured"
                                                text="New window"
                                                value="_blank"/>
                                        </items>
                                    </cta2target>
                                    <cta2text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Learn more Text"
                                        name="./cta2text"/>
                                    <cta2link
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Learn more Url"
                                        name="./cta2link"
                                        required="{Boolean}false"
                                        rootPath="/content/edc"/>
                                </items>
                            </well>
                        </items>
                    </cta2>
                    <cta3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <heading
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/text"
                                fieldDescription="CTA Send a Question Label"
                                text="CTA Send a Question"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <layout
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <cta3target
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Send a Question Target"
                                        name="./cta3target">
                                        <items jcr:primaryType="nt:unstructured">
                                            <existing
                                                jcr:primaryType="nt:unstructured"
                                                text="Existing window"
                                                value="_self"/>
                                            <new
                                                jcr:primaryType="nt:unstructured"
                                                text="New window"
                                                value="_blank"/>
                                        </items>
                                    </cta3target>
                                    <cta3text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Send a Question Text"
                                        name="./cta3text"/>
                                    <cta3link
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Send a Question Url"
                                        name="./cta3link"
                                        required="{Boolean}false"
                                        rootPath="/content/edc"/>
                                    <sendQuestionFormButtonEnTxt
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Analytics Send a Question Form Button English Text i.e. 'Send a Question'"
                                        fieldLabel="Analytics Send a Question Form Button English Text"
