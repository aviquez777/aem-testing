<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <questionType
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                fieldDescription="Determines the Question Type"
                fieldLabel="Question Type"
                name="./questionType"
                required="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                   <pleaseSelect
                        jcr:primaryType="nt:unstructured"
                        text="Please Select"
                        value=""/>
                   <firstName
                        jcr:primaryType="nt:unstructured"
                        text="First Name (Input Text)"
                        value="firstName"/>
                   <lastName
                        jcr:primaryType="nt:unstructured"
                        text="Last Name (Input Text)"
                        value="lastName"/>
                   <tradeStatus
                        jcr:primaryType="nt:unstructured"
                        text="Export / Trade Status (Single Select)"
                        value="tradeStatus"/>
                   <painPoint
                        jcr:primaryType="nt:unstructured"
                        text="Export Challenges / Pain Points (Multi Select)"
                        value="painPoint"/>
                   <annualSales
                        jcr:primaryType="nt:unstructured"
                        text="Annual Sales (Single Select)"
                        value="annualSales"/>
                    <salesTiming
                         jcr:primaryType="nt:unstructured"
                         text="Upcoming Business Sale (Single Select)"
                         value="salesTiming"/>
                   <fiLoan
                        jcr:primaryType="nt:unstructured"
                        text="Financial Institution Loan? (Yes / No Radio)"
                        value="fiLoan"/>
                   <covidWC
                        jcr:primaryType="nt:unstructured"
                        text="Covid Working Capital? (Yes / No Radio)"
                        value="covidWC"/>
                </items>
            </questionType>
        </items>
    </content>
</jcr:root>
