<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items
            jcr:primaryType="nt:unstructured">
            <companyinfotitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company info title"
                fieldDescription="Black text part of the company info title"
                name="./companyinfotitle"
                required="{Boolean}true"/>
            <companyinfobluetitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company info blue title"
                fieldDescription="Blue text part of the company info title"
                name="./companyinfobluetitle"
                required="{Boolean}true"/>
            <companyinfodescription
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company info description"
                fieldDescription="Text under Company info title"
                name="./companyinfodescription"
                required="{Boolean}true"/>
            <inclusivetrade
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/edc/components/common/inclusivetrade/cq:dialog/content"/>      
        </items>
    </content>
</jcr:root>
