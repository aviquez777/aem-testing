                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" 
                                    fieldLabel="Declaration text"
                                    fieldDescription="Declaration list item text"
                                    required="{Boolean}true"
                                    name="./felistitem" />
                            </items> 
                        </field> 
                    </felist>
                    <fetext2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Further declarations heading paragraph"
                        fieldLabel="Further declarations heading paragraph"
                        required="{Boolean}true"
                        name="./fetext2"/>
                    <felist2 jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield" 
                        fieldDescription="Further declarations list items" 
                        fieldLabel="Further declarations list items" 
                        composite="{Boolean}true"
                        required="{Boolean}true"> 
                        <field jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container" 
                                name="./felist2"> 
                            <items jcr:primaryType="nt:unstructured">
                                <felist2item jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" 
