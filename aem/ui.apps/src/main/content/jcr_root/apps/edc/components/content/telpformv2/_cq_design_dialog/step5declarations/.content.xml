                                <ielistitem jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" 
                                    fieldLabel="Declaration text"
                                    fieldDescription="Declaration list item text"
                                    required="{Boolean}true"
                                    name="./ielistitem" />
                            </items> 
                        </field> 
                    </ielist>
                    <ietext2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
