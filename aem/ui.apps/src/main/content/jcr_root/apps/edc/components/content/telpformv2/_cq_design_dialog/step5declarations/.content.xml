<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <directexporter jcr:primaryType="nt:unstructured"
                jcr:title="Direct exporter declaration"
                sling:resourceType="granite/ui/components/foundation/form/fieldset">
                <items jcr:primaryType="nt:unstructured">
                    <detext
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Declarations heading paragraph"
                        fieldLabel="Declarations heading paragraph"
                        required="{Boolean}true"
                        name="./detext"/>
                    <delist jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield" 
                        fieldDescription="Declarations list items" 
                        fieldLabel="Declarations list items" 
                        composite="{Boolean}true"
                        required="{Boolean}true"> 
                        <field jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container" 
                                name="./delist"> 
                            <items jcr:primaryType="nt:unstructured">
                                <delistitem jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" 
                                    fieldLabel="Declaration text"
                                    fieldDescription="Declaration list item text"
                                    required="{Boolean}true"
                                    name="./delistitem" />
                            </items> 
                        </field> 
                    </delist>
                    <detext2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Further declarations heading paragraph"
                        fieldLabel="Further declarations heading paragraph"
                        required="{Boolean}true"
                        name="./detext2"/>
                    <delist2 jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield" 
                        fieldDescription="Further declarations list items" 
                        fieldLabel="Further declarations list items" 
                        composite="{Boolean}true"
                        required="{Boolean}true"> 
                        <field jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container" 
                                name="./delist2"> 
                            <items jcr:primaryType="nt:unstructured">
                                <delist2item jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield" 
                                    fieldLabel="Declaration text"
                                    fieldDescription="Declaration text (if last, must be primary country of export text)"
                                    required="{Boolean}true"
                                    name="./delist2item" />
                            </items> 
                        </field> 
                    </delist2>
                </items>
            </directexporter>
            <indirectexporter jcr:primaryType="nt:unstructured"
