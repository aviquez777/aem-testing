<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Step 3 (Canadian flow)"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <remitinfoentitylabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info entity name field label"
                name="./remitinfoentitylabel"
                required="{Boolean}true"/>
            <remitinfoentityplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info entity name placeholder"
                name="./remitinfoentityplaceholder"
                required="{Boolean}false"/>
            <remitinfotransitnumlabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info transit number field label"
                name="./remitinfotransitnumlabel"
                required="{Boolean}true"/>
            <remitinfotransitnumplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info transit number placeholder"
                name="./remitinfotransitnumplaceholder"
                required="{Boolean}false"/>
            <remitinfoinstnumlabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info institution number field label"
                name="./remitinfoinstnumlabel"
                required="{Boolean}true"/>
            <remitinfoinstnumplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info institution number placeholder"
                name="./remitinfoinstnumplaceholder"
                required="{Boolean}false"/>
            <remitinfoaccnumlabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info account number field label"
                name="./remitinfoaccnumlabel"
                required="{Boolean}true"/>
            <remitinfoaccnumplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info account number placeholder"
                name="./remitinfoaccnumplaceholder"
                required="{Boolean}false"/>
            <remitinfoemaillabel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info email field label"
                name="./remitinfoemaillabel"
                required="{Boolean}true"/>
            <remitinfoemailplaceholder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Remit info email placeholder"
                name="./remitinfoemailplaceholder"
                required="{Boolean}false"/>
        </items>
    </content>
</jcr:root>