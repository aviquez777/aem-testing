<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <step3
                jcr:primaryType="nt:unstructured"
                jcr:title="Step 1"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items
                            jcr:primaryType="nt:unstructured">
                            <step3Title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Title"
                                fieldDescription="Right of Step 3 of 4 i.e Financial institution account manager information"
                                name="./step3Title"
                                required="{Boolean}true"/>
                           <step3Description
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Description"
                               fieldDescription="Description immediately under the title "
                               name="./step3Description"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </step3Description>
                           <managerTitle jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Financial institution account manager contact information"
                                required="{Boolean}true"
                                fieldLabel="Financial institution account manager title"
                                name="./managerTitle"/>
                           <managerOptDesc jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Optional Description"
                               fieldDescription="Optional description under the section title Financial institution account manager contact information"
                               name="./managerOptDesc"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                            </managerOptDesc>
                            <fiModalIntro
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/text"
                                class="coral-Heading--4"
                                text="Financial Institution Modal"/>
                            <fiModalTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Financial Institution Modal Title"
                                fieldDescription="Financial Institution Modal Title"
                                name="./fiModalTitle"
                                required="{Boolean}false"/>
                           <fiModalDescription
                               jcr:primaryType="nt:unstructured"
                               sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                               fieldLabel="Financial Institution Modal Description"
                               fieldDescription="Financial Institution Modal Description."
                               name="./fiModalDescription"
                               useFixedInlineToolbar="{Boolean}true">
                               <rtePlugins jcr:primaryType="nt:unstructured">
                                   <format
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <justify
                                       jcr:primaryType="nt:unstructured"
                                       features="*"/>
                                   <links
                                       jcr:primaryType="nt:unstructured"
                                       features="[modifylink,unlink]"/>
                               </rtePlugins>
                               <uiSettings jcr:primaryType="nt:unstructured">
                                   <cui jcr:primaryType="nt:unstructured">
                                       <inline
                                           jcr:primaryType="nt:unstructured"
                                           toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                           <popovers jcr:primaryType="nt:unstructured">
                                               <justify
                                                   jcr:primaryType="nt:unstructured"
                                                   items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                   ref="justify"/>
                                           </popovers>
                                       </inline>
                                   </cui>
                               </uiSettings>
                           </fiModalDescription>
                            <fiLovApiLoadError
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Lov API Error text (BCAP / MMG)"
                                fieldDescription="If the API is unavailable, must display an error message on Step 1"
                                name="./fiLovApiLoadError"
                                required="{Boolean}false"/>
                            <wellin
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <extraFi
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/text"
                                        class="coral-Heading--4"
                                        text="Extra Financial Information (Mid Market BCAP Only)"/>
                                    <extraFiTitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial Title"
                                        fieldDescription="Select all the lenders that are involved in financing" 
                                        required="{Boolean}false"
                                        name="./extraFiTitle"/>
                                    <extraFiLabelYes
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial Radio Button Label for yes"
                                        fieldDescription="Extra Financial Radio Button Label for yes"
                                        required="{Boolean}false"
                                        name="./extraFiLabelYes"/>
                                    <extraFiLabelNo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial Radio Button Label for no"
                                        fieldDescription="Extra Financial Radio Button Label for no"
                                        required="{Boolean}false"
                                        name="./extraFiLabelNo"/>
                                    <extraFiDropdownLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial Dropdown Label"
                                        fieldDescription="Select all the lenders that are involved in financing"
                                        required="{Boolean}false"
                                        name="./extraFiDropdownLabel"/>
                                    <extraFiNotListedOption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="'My FI is not on this list' option item label"
                                        fieldDescription="'My FI is not on this list' option item label"
                                        required="{Boolean}false"
                                        name="./extraFiNotListedOption"/>
                                    <extraFiNotListedLabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial 'Other if not listed' Label"
                                        fieldDescription="Please list financial institutions involved in your lending not noted above"
                                        required="{Boolean}false"
                                        name="./extraFiNotListedLabel"/>
                                    <extraFiNotListedPh
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Extra Financial 'Other if not listed' Placeholder"
                                        fieldDescription="Extra Financial 'Other if not listed' Placeholder"
                                        required="{Boolean}false"
                                        name="./extraFiNotListedPh"/>
                                </items>
                            </wellin>
                        </items>
                    </column>
                </items>
            </step3>
        </items>
    </content>
</jcr:root>
