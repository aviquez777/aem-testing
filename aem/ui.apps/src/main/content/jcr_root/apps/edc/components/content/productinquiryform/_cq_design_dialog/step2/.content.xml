<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items
            jcr:primaryType="nt:unstructured">
            <step2navtitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 2 title on steps nav"
                name="./step2navtitle"
                required="{Boolean}true"/>
            <step2navsubtitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 2 subtitle on steps nav"
                name="./step2navsubtitle"
                required="{Boolean}true"/>
            <companyname
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company name field label"
                name="./companyname"
                required="{Boolean}true"/>
            <companynameph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company name field placeholder"
                name="./companynameph"
                required="{Boolean}false"/>
            <industry
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Industry sector field label"
                name="./industry"
                required="{Boolean}true"/>
            <industryddph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Industry sector field placeholder"
                name="./industryddph"
                required="{Boolean}true"/>
            <employees
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Number of employees field label"
                name="./employees"
                required="{Boolean}true"/>
            <employeesddph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Number of employees field placeholder"
                name="./employeesddph"
                required="{Boolean}true"/>
            <openpayment
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Open payment for purchase offers field label"
                name="./openpayment"
                required="{Boolean}true"/>
            <tradestatus
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Trade status (company's activities) field label"
                name="./tradestatus"
                required="{Boolean}true"/>
            <tradestatusddph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Trade status (company's activities) field placeholder"
                name="./tradestatusddph"
                required="{Boolean}true"/>
        </items>
    </content>
</jcr:root>
