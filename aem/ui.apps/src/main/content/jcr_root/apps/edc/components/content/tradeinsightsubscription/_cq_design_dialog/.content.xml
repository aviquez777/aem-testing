<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Trade Insight Subscription"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <header
                jcr:primaryType="nt:unstructured"
                jcr:title="Trade Insight Subscription"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ctatext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Text for the CTA button"
                                fieldLabel="Text for the CTA button"
                                name="./ctatext"/>
                            <logoimage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/include"
                                name="./logoimage"
                                path="/apps/edc/components/common/dialogs/cq:dialog/content/items/column/items/image"/>
                            <cookiepath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="The URL visitors will be directed to after clicking the Cookie link."
                                fieldLabel="Cookie disclaimer url"
                                name="./cookiepath"
                                required="{Boolean}false"
                                rootPath="/content/edc"/>
                            <successmessage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Success message after subscription"
                                fieldLabel="Success message"
                                name="./successmessage"/>
                            <cookiedisclaimer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Text for Cookie Disclaimer"
                                fieldLabel="Cookie Disclaimer"
                                name="./cookiedisclaimer"/>
                            <cookielabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Text for the cookie disclaimer link"
                                fieldLabel="cookie link "
                                name="./cookielabel"/>
                            <title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Title for the subscription component"
                                fieldLabel="Title"
                                name="./title"/>
                            <emailemptylabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Text for the email form field (greyed)"
                                fieldLabel="email empty text"
                                name="./emailemptylabel"/>
                        </items>
                    </column>
                </items>
            </header>
        </items>
    </content>
</jcr:root>
