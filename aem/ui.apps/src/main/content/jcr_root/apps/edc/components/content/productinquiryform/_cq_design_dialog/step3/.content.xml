<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items
            jcr:primaryType="nt:unstructured">
            <step3navtitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 3 title on steps nav"
                name="./step3navtitle"
                required="{Boolean}true"/>
            <step3navsubtitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Step 3 subtitle on steps nav"
                name="./step3navsubtitle"
                required="{Boolean}true"/>
            <firstname
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="First name field label"
                name="./firstname"
                required="{Boolean}true"/>
            <firstnameph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="First name field placeholder"
                name="./firstnameph"
                required="{Boolean}false"/>
            <lastname
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Last name field label"
                name="./lastname"
                required="{Boolean}true"/>
            <lastnameph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Last name field placeholder"
                name="./lastnameph"
                required="{Boolean}false"/>
            <jobtitle
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Job title field label"
                name="./jobtitle"
                required="{Boolean}true"/>
            <jobtitleph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Job title field placeholder"
                name="./jobtitleph"
                required="{Boolean}false"/>
            <businessemail
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Business email address field label"
                name="./businessemail"
                required="{Boolean}true"/>
            <businessemailph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Business email address field placeholder"
                name="./businessemailph"
                required="{Boolean}false"/>
            <phone
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company phone field label"
                name="./phone"
                required="{Boolean}true"/>
            <phoneph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company phone field placeholder"
                name="./phoneph"
                required="{Boolean}false"/>
            <phoneext
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Phone extension field label"
                name="./phoneext"
                required="{Boolean}true"/>
            <phoneextph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Phone extension field placeholder"
                name="./phoneextph"
                required="{Boolean}false"/>
            <country
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company country field label"
                name="./country"
                required="{Boolean}true"/>
            <countryddph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Company country field placeholder"
                name="./countryddph"
                required="{Boolean}true"/>
            <address
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Address field label"
                name="./address"
                required="{Boolean}true"/>
            <addressph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Address field placeholder"
                name="./addressph"
                required="{Boolean}false"/>
            <address2
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Address 2 field label"
                name="./address2"
                required="{Boolean}true"/>
            <address2ph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Address 2 field placeholder"
                name="./address2ph"
                required="{Boolean}false"/>
            <province
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Province field label"
                name="./province"
                required="{Boolean}true"/>
            <state
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="State field label"
                name="./state"
                required="{Boolean}true"/>
            <provinceph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Province/state field placeholder"
                name="./provinceph"
                required="{Boolean}false"/>
            <city
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="City field label"
                name="./city"
                required="{Boolean}true"/>
            <cityph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="City field placeholder"
                name="./cityph"
                required="{Boolean}false"/>
            <postal
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Postal code field label"
                name="./postal"
                required="{Boolean}true"/>
            <postalph
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Postal code field placeholder"
                name="./postalph"
                required="{Boolean}false"/>
            <casl
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                fieldLabel="Casl consent checkbox label"
                name="./casl"
                useFixedInlineToolbar="{Boolean}true">
                <rtePlugins jcr:primaryType="nt:unstructured">
                    <format
                        jcr:primaryType="nt:unstructured"
                        features="*"/>
                    <justify
                        jcr:primaryType="nt:unstructured"
                        features="*"/>
                    <links
                        jcr:primaryType="nt:unstructured"
                        features="[modifylink,unlink]"/>
                </rtePlugins>
                <uiSettings jcr:primaryType="nt:unstructured">
                    <cui jcr:primaryType="nt:unstructured">
                        <inline
                            jcr:primaryType="nt:unstructured"
                            toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                            <popovers jcr:primaryType="nt:unstructured">
                                <justify
                                    jcr:primaryType="nt:unstructured"
                                    items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                    ref="justify"/>
                            </popovers>
                        </inline>
                    </cui>
                </uiSettings>
            </casl>
        </items>
    </content>
</jcr:root>
