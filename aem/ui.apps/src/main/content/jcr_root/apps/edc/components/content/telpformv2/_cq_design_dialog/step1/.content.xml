                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="CDIA Review Text"
                fieldDescription="Label for CDIA Review Label text"
                name="./cdiaReviewLabel"
                required="{Boolean}true"/>
          <hqTitle jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/heading"
                level="{Long}4"
                text="Headquarters information is configured on Dialog"/>
           <companyAddress1
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for Headquarters Address Line 1"
