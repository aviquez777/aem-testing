                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for Canada's Province drop down"
                fieldDescription="Label for Canada's Province drop down"
                name="./companyProvince"
                required="{Boolean}true"/>
            <companyProvincePh jcr:primaryType="nt:unstructured"
