                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for CDIA radios question 2 yes"
                fieldLabel="Label for CDIA radios question 2 yes"
                required="{Boolean}true"
                name="./forFinancingYes"/>
            <forFinancingNo jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for CDIA radios question 2 no"
                fieldLabel="Label for CDIA radios question 2 no"
                required="{Boolean}true"
