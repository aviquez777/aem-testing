<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <contactFirstName jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for First Name"
                fieldLabel="Label for First Name"
                required="{Boolean}true"
                name="./contactFirstName"/>
            <contactFirstNamePh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for First Name"
                fieldLabel="Placeholder for First Name"
                required="{Boolean}false"
                name="./contactFirstNamePh"/>
            <contactLastName jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for Last Name"
                fieldLabel="Label for Last Name"
                required="{Boolean}true"
                name="./contactLastName"/>
            <contactLastNamePh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Last Name"
                fieldLabel="Placeholder for Last Name"
                required="{Boolean}false"
                name="./contactLastNamePh"/>
            <contactEmailAddress jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for Email address"
                fieldLabel="Label for Email address"
                required="{Boolean}true"
                name="./contactEmailAddress"/>
            <contactEmailAddressPh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Email address"
                fieldLabel="Placeholder for Email address"
                required="{Boolean}false"
                name="./contactEmailAddressPh"/>
            <businessPhoneNumber jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for Phone Number"
                fieldLabel="Label for Phone Number"
                required="{Boolean}true"
                name="./businessPhoneNumber"/>
            <businessPhoneNumberPh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Phone Number"
                fieldLabel="Placeholder for Phone Number"
                required="{Boolean}false"
                name="./businessPhoneNumberPh"/>
            <companyTitle jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/heading"
                level="{Long}4"
                text="Company Title is configured on Dialog"/>
            <legalName jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for legal name of company"
                fieldLabel="Label for Legal name of company"
                required="{Boolean}true"
                name="./legalName"/>
            <legalNamePh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Legal name of company"
                fieldLabel="Placeholder for Legal name of company"
                required="{Boolean}false"
                name="./legalNamePh"/>
            <tradeName jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Label for Trade name (if applicable)"
                fieldLabel="Label for Trade name (if applicable)"
                required="{Boolean}true"
                name="./tradeName"/>
