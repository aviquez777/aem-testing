<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"  xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Supplier Registration Form"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <formTab jcr:primaryType="nt:unstructured"
                        jcr:title="Form"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <form
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/form/content"/>
                        </items>
                    </formTab>
                    <step1Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 1"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step1/content"/>
                        </items>
                    </step1Tab>
                    <step2Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step2/content"/>
                        </items>
                    </step2Tab>
                    <step2CanTab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2 (Canadian flow)"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3can
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step2-canadian/content"/>
                        </items>
                    </step2CanTab>
                    <step2UsTab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2 (US flow)"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3can
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step2-us/content"/>
                        </items>
                    </step2UsTab>
                    <step2IntTab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 2 (International flow)"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3int
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step2-international/content"/>
                        </items>
                    </step2IntTab>
                    <step3Tab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step3/content"/>
                        </items>
                    </step3Tab>
                    <step3CanTab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3 (Canadian flow)"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3can
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step3-canadian/content"/>
                        </items>
                    </step3CanTab>
                    <step3IntTab jcr:primaryType="nt:unstructured"
                        jcr:title="Step 3 (International flow)"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <step3int
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/include"
                                path="edc/components/content/supplierregistrationform/cq:design_dialog/step3-international/content"/>
                        </items>
                    </step3IntTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>