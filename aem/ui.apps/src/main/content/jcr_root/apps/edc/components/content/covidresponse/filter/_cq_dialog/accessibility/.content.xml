<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <general
                jcr:primaryType="nt:unstructured"
                jcr:title="accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items
                            jcr:primaryType="nt:unstructured">
                                <searchResultsAria
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Covid Search Results' Aria text"
                                    fieldLabel="'Covid Search Results' Aria text"
                                    required="{Boolean}true"
                                    name="./searchResultsAria"/>
                                <btnClose
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Close' Button Aria text"
                                    fieldLabel="'Close'Button Aria text"
                                    required="{Boolean}true"
                                    name="./btnClose"/>
                               <hideOptions
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Hide Options' Aria text"
                                    fieldLabel="'Hide Options' Aria text"
                                    required="{Boolean}true"
                                    name="./hideOptions"/>
                               <showOptions
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Show Options' Aria text"
                                    fieldLabel="'Show Options' Aria text"
                                    required="{Boolean}true"
                                    name="./showOptions"/>
                               <filterDataCloseLabel
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Remove tag' Aria text"
                                    fieldLabel="'Remove tag' Aria text"
                                    required="{Boolean}true"
                                    name="./filterDataCloseLabel"/>
                               <filterDataTitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldDescription="'Covid Filter Tags' Aria text"
                                    fieldLabel="'Covid Filter Tags' Aria text"
                                    required="{Boolean}true"
                                    name="./filterDataTitle"/>
                        </items>
                    </column>
                </items>
            </general>
        </items>
    </content>
</jcr:root>
