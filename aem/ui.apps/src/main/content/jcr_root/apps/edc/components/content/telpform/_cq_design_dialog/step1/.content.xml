            <companyProvinceAltPh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for USA States drop down"
                fieldLabel="Placeholder for USA States drop down"
                required="{Boolean}true"
