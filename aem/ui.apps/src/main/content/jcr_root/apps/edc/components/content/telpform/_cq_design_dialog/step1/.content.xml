            <ceoFirstNamePh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for CEO First Name"
                fieldLabel="Placeholder for CEO First Name"
                required="{Boolean}false"
                name="./ceoFirstNamePh"/>
            <ceoLastName
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for for CEO Last Name"
