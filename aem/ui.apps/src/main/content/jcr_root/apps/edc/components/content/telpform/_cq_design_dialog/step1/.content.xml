                fieldDescription="Placeholder for Headquarters Postal/Zip code"
                fieldLabel="Placeholder for Headquarters Postal/Zip code"
                required="{Boolean}false"
                name="./companyPostalPh"/>
            <ceoFirstName
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for CEO First Name"
                fieldDescription="Label for CEO First Name"
