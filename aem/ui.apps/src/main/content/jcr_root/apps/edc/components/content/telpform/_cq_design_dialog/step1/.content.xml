                name="./companyCountry"
                required="{Boolean}true"/>
            <companyCountryPh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Headquarters Country"
                fieldLabel="Placeholder for Headquarters Country"
                required="{Boolean}true"
                name="./companyCountryPh"/>
           <companyProvince
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for Canada's Province drop down"
                fieldDescription="Label for Canada's Province drop down"
                name="./companyProvince"
                required="{Boolean}true"/>
            <companyProvincePh jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldDescription="Placeholder for Canada's Province drop down"
                fieldLabel="Placeholder for Canada's Province drop down"
                required="{Boolean}true"
                name="./companyProvincePh"/>
            <companyProvinceAlt
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                fieldLabel="Label for USA States drop down"
                fieldDescription="Label for  USA States drop down"
                name="./companyProvinceAlt"
