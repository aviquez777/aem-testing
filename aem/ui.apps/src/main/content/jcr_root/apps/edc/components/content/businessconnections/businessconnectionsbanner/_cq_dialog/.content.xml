<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" 
xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Business Connections - Event Banner"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <generalTab jcr:primaryType="nt:unstructured"
                jcr:title="Banner Info"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/include"
                        path="edc/components/content/businessconnections/businessconnectionsbanner/cq:dialog/general/content"/>
                </items>
            </generalTab>
            <imagesTab jcr:primaryType="nt:unstructured"
                jcr:title="Images"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <images
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/include"
                        path="edc/components/content/businessconnections/businessconnectionsbanner/cq:dialog/images/content"/>
                </items>
            </imagesTab>
            <gatingInfoTab jcr:primaryType="nt:unstructured"
                jcr:title="Gating Info"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <gatingInfo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/include"
                        path="edc/components/content/businessconnections/businessconnectionsbanner/cq:dialog/gatinginfo/content"/>
                </items>
            </gatingInfoTab>
        </items>
    </content>
</jcr:root>