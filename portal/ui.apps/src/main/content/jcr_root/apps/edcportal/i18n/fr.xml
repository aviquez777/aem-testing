        sling:message="Retour au tableau de bord"/>
    <link-text-manage-profile
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="link-text-manage-profile"
        sling:message="Gérer mon profile"/>
    <label-text-other
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="label-text-other"
        sling:message="Autre"/>
    <label-text-profile
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="label-text-profile"
        sling:message="Profil"/>
    <label-text-all-fields-required-reg
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="label-text-all-fields-required-reg"
        sling:message="Tous les champs sont obligatoires."/>
    <label-text-all-fields-required
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="label-text-all-fields-required"
