        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="label-text-i-understand-terms-conditions-2"
        sling:message="d'EDC et j'y consens."/>
    <link-text-logout
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="link-text-logout"
