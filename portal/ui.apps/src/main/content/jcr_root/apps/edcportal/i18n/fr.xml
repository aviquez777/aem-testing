    <button-text-next
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="button-text-next"
        sling:message="Suivant"/> 
    <button-text-submit
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="button-text-submit"
        sling:message="Soumettre"/> 
